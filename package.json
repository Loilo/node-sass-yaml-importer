{
  "name": "node-sass-yaml-importer",
  "version": "4.0.1",
  "description": "Allows importing YAML in sass files parsed by node-sass.",
  "keywords": [
    "importer",
    "node-sass",
    "sass",
    "yaml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Loilo/node-sass-yaml-importer.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/node-sass-yaml-importer.js",
  "scripts": {
    "compile": "mkdirp dist && babel -o dist/node-sass-yaml-importer.js src/index.js",
    "prepublishOnly": "npm run compile",
    "pretest": "npm run compile",
    "test": "mocha"
  },
  "dependencies": {
    "is-plain-object": "^2.0.4",
    "js-yaml": "^3.8.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "chai": "^4.1.2",
    "mkdirp": "^0.5.1",
    "mocha": "^7.1.2",
    "node-sass": ">=3.5.3"
  },
  "peerDependencies": {
    "node-sass": ">=3.5.3"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
